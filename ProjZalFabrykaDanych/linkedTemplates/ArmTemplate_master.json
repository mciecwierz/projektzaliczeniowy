{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ProjZalFabrykaDanych"
		},
		"DataLakeGen2Connection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeGen2Connection'"
		},
		"SourceMetadata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SourceMetadata'"
		},
		"ProjZalKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ProjZal2020KeyVault.vault.azure.net/"
		},
		"S3Connection_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAIDNCNSE65LTVUTYA"
		},
		"DataLakeGen2Connection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projektzaldatalake2020.dfs.core.windows.net"
		},
		"SourceMetadata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projektzaldatalake2020.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLakeGen2Connection_accountKey": {
						"value": "[parameters('DataLakeGen2Connection_accountKey')]"
					},
					"SourceMetadata_accountKey": {
						"value": "[parameters('SourceMetadata_accountKey')]"
					},
					"ProjZalKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ProjZalKeyVault_properties_typeProperties_baseUrl')]"
					},
					"DataLakeGen2Connection_properties_typeProperties_url": {
						"value": "[parameters('DataLakeGen2Connection_properties_typeProperties_url')]"
					},
					"SourceMetadata_properties_typeProperties_url": {
						"value": "[parameters('SourceMetadata_properties_typeProperties_url')]"
					},
					"S3Connection_properties_typeProperties_accessKeyId": {
						"value": "[parameters('S3Connection_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}